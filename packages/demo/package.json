{
  "name": "@elora-cloud/vue-grid-layout-demo",
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "build": "cross-env NODE_ENV=production vite build && vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:es": "eslint \"src/**/*.{js,ts,vue}\" --fix --color",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --color --fix",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@elora-cloud/vue-grid-layout": "workspace:*",
    "ant-design-vue": "catalog:",
    "cross-env": "catalog:",
    "echarts": "catalog:",
    "uuid": "catalog:",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "vite": "catalog:",
    "vue-template-compiler": "catalog:",
    "vue-tsc": "catalog:"
  }
}

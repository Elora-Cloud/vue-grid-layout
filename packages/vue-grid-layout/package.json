{
  "name": "@elora-cloud/vue-grid-layout",
  "version": "1.0.3",
  "type": "module",
  "description": "Vue3 grid layout with resize, drag and responsive",
  "keywords": [
    "grid",
    "grid-layout",
    "layout",
    "resize",
    "drag",
    "vue3",
    "vue",
    "composition-api",
    "typescript"
  ],
  "author": {
    "name": "mumulx",
    "email": "mumu_lx@163.com"
  },
  "repository": {
    "type": "gitee",
    "url": "https://gitee.com/Elora_Cloud/vue-grid-layout.git",
    "directory": "packages/vue-grid-layout"
  },
  "bugs": {
    "url": "https://gitee.com/Elora_Cloud/vue-grid-layout/issues",
    "email": "mumu_lx@163.com"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "license": "MIT",
  "main": "dist/vue-grid-layout.js",
  "module": "dist/vue-grid-layout.js",
  "types": "dist/components/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/vue-grid-layout.js",
      "require": "./dist/vue-grid-layout.cjs",
      "default": "./dist/vue-grid-layout.js"
    },
    "./styles": {
      "import": "./dist/style.css",
      "default": "./dist/style.css"
    },
    "./types": {
      "import": "./dist/types/index.d.ts",
      "require": "./dist/types/index.d.ts",
      "default": "./dist/types/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/components/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build:watch": "vite build --watch",
    "build": "cross-env NODE_ENV=production vite build && vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "lint:es": "eslint \"src/**/*.{js,ts,vue}\" --fix --color",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --color --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "publish:yalc": "pnpm build && yalc publish && yalc push"
  },
  "dependencies": {
    "@interactjs/actions": "^1.10.20",
    "@interactjs/auto-scroll": "^1.10.20",
    "@interactjs/auto-start": "^1.10.20",
    "@interactjs/dev-tools": "^1.10.20",
    "@interactjs/interact": "^1.10.20",
    "@interactjs/modifiers": "^1.10.20",
    "@interactjs/core": "^1.10.20",
    "@interactjs/utils": "^1.10.20",
    "interactjs": "^1.10.20",
    "element-resize-detector": "^1.2.4",
    "mitt": "^3.0.1"
  },
  "peerDependencies": {
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@types/element-resize-detector": "^1.1.5",
    "@babel/types": "^7.23.0",
    "@types/jest": "^29.5.7",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "cross-env": "^7.0.3",
    "jsdom": "^22.1.0",
    "sass": "^1.69.5",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "vue-eslint-parser": "^9.3.2",
    "vue-template-compiler": "^2.7.15",
    "vue-tsc": "^1.8.22"
  }
}
